{
  "name": "webpurify",
  "version": "2.0.1",
  "description": "This provides an easy module for interacting with the WebPurify API",
  "main": "dist/index.js",
  "scripts": {
    "postinstall": "npm run-script build",
    "test": "./node_modules/.bin/mocha --require babel-register",
    "build": "./node_modules/.bin/babel src --out-dir dist",
    "build-docs": "./node_modules/.bin/esdoc",
    "clean": "rm -rf ./node_modules ./dist"
  },
  "repository": "https://github.com/mileszim/webpurify",
  "keywords": [
    "webpurify",
    "api"
  ],
  "author": "Miles Zimmerman",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "esdoc": "^1.1.0",
    "esdoc-standard-plugin": "^1.0.0",
    "mocha": "^5.1.1",
    "nock": "^9.2.5",
    "sinon": "^4.5.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0"
  }
}
